<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>

</head>
<body ng-app="myApp">
       <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("i - Mahiri", "Index", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@*@Html.ActionLink("Home", "Index", "Home")*@</li>
                </ul>
            </div>
        </div>
    </div>
    
    
    <div class="body-content">
        @RenderBody()
        <hr />
        <footer>
            <p></p>
        </footer>
    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    
    <script src="~/Scripts/angular.min.js"></script>
    <script type="text/javascript" src="https://www.google.com/jsapi"></script>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDUkwO_jjlDd8f86sbzcIQ51GzTPIyb5D0"></script>
    <script src="~/Scripts/Angular_Custom_App/googleMaps.js"></script>

    <script src="~/Scripts/Angular_Custom_App/app.js"></script>
    <script src="~/Scripts/Angular_Custom_App/mainController.js"></script>
    @*<script src="~/Scripts/Angular_Custom_App/graphing.js"></script>*@



</body>
</html>
